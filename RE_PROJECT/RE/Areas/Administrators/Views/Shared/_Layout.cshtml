    <!--custom navbar right css-->
    <style>
        .overlay {
            height: 100%;
            width: 0;
            position: fixed;
            z-index: 1;
            top: 0;
            left: 0;
            background-color: rgb(0,0,0);
            background-color: rgba(0,0,0, 0.90);
            overflow-x: hidden;
            transition: 0.5s;
        }

        .overlay-content {
            position: relative;
            top: 0%;
            width: 100%;
            text-align: left;
            margin-top: 30px;
        }

        .overlay a {
            padding-left: 5px;
            padding: 5px;
            text-decoration: none;
            font-size: 15px;
            color: white;
            display: block;
            transition: 0.3s;
        }

            .overlay a:hover, .overlay a:focus {
                color: orangered;
                
            }

        .overlay {
            position: absolute;
            top: 20px;
            right: 45px;
            font-size: 60px;
        }

        .closebtn {
            top: 50px;
            position: absolute;
            right: 45px;
            font-size: 60px;
        }
    </style>
